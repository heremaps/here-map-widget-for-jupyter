{
  "name": "@here/map-widget-for-jupyter",
  "version": "1.1.4",
  "description": "Use HERE Maps API for JavaScript in your Jupyter Notebook.",
  "author": {
    "name": "HERE Europe B.V.",
    "url": "https://here.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "ssh://git@github.com:heremaps/here-map-widget-for-jupyter.git",
    "directory": "js"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js",
    "lib/**/*.css",
    "dist/*.css",
    "lib/**/*.png",
    "dist/*.png",
    "lib/**/*.gif",
    "dist/*.gif",
    "dist/*.map",
    "LICENSE"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../here_map_widget/labextension/ && rimraf ../here_map_widget/nbextension",
    "prepublish": "webpack --mode=production",
    "build": "webpack --mode=production",
    "build:extensions": "webpack --mode=production && jupyter labextension build .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "css-loader": "^7.1.1",
    "file-loader": "^6.2.0",
    "install": "^0.13.0",
    "rimraf": "^5.0.5",
    "style-loader": "^4.0.0",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "@jupyterlab/builder": "^3.6.7"
  },
  "dependencies": {
    "@here/maps-api-for-javascript": "^1.54.0",
    "@jupyter-widgets/base": "^6",
    "@lumino/messaging": "^2.0.1",
    "@lumino/widgets": "^2.3.2",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "@turf/centroid": "^6.5.0",
    "@turf/helpers": "^6.5.0",
    "flat": "^6.0.1",
    "lodash": "^4.17.21",
    "run": "^1.5.0",
    "stream-browserify": "^3.0.0"
  },
  "jupyterlab": {
    "extension": "lib/labplugin",
    "outputDir": "../here_map_widget/labextension",
    "webpackConfig": "../jlab/webpack.config.js",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  }
}
